                /*%FSM<STATE "Load_Medical">*/
                class Load_Medical
                {
                        name = "Load_Medical";
                        itemno = 91;
                        init = /*%FSM<STATEINIT""">*/"if !(_medical isEqualTo []) then " \n
                         "{" \n
                         "	player setVariable [""med_isDead"",(_medical select 0),true];" \n
                         "	player setVariable [""med_unconscious"", (_medical select 1), true];" \n
                         "	player setVariable [""med_infected"",(_medical select 2),true];" \n
                         "	player setVariable [""med_injured"",(_medical select 3),true];" \n
                         "	player setVariable [""med_inPain"",(_medical select 4),true];" \n
                         "	player setVariable [""med_isCardiac"",(_medical select 5),true];" \n
                         "	player setVariable [""med_lowBlood"",(_medical select 6),true];" \n
                         "	player setVariable [""med_bleedingLevel"",(_medical select 7),true];" \n
                         "	player setVariable [""med_BloodQty"",(_medical select 8),false]; //Local" \n
                         "	" \n
                         "	//Add fractures" \n
                         "	_fractures = (_medical select 9);" \n
                         "       if !(_fractures isEqualTo []) then" \n
                         "	 {" \n
                         "		_fractures params [""_legs"",""_arms""];" \n
                         "		r_hit_legs = _legs;" \n
                         "		r_hit_hands = _arms;" \n
                         "		if (_legs > 0.9) then {" \n
                         "			r_fracture_legs = true;" \n
                         "			r_hit_legs = 1;" \n
                         "		};" \n
                         "		if (_arms > 0.9) then {" \n
                         "			r_fracture_arms = true;" \n
                         "			r_hit_hands = 1;" \n
                         "		};" \n
                         "		player setHitPointDamage [""HitLegs"", r_hit_legs];" \n
                         "		player setHitPointDamage [""HitHands"", r_hit_hands];" \n
                         "" \n
                         "	} else {" \n
                         "		player setHitPointDamage [""HitLegs"", 0];" \n
                         "		player setHitPointDamage [""HitHands"", 0];" \n
                         "	};" \n
                         "" \n
                         "	player setVariable [""messing"",(_medical select 10),false];" \n
                         "	player setVariable [""unconsciousTime"",(_medical select 11),false];" \n
                         "" \n
                         "} else {" \n
                         "	//Reset Fractures" \n
                         "	player setHitPointDamage [""HitLegs"", 0];" \n
                         "	player setHitPointDamage [""HitHands"", 0];" \n
                         "	player setVariable [""med_injured"",false,true];" \n
                         "	player setVariable [""med_inPain"",false,true];" \n
                         "	player setVariable [""messing"",[0,0],false];" \n
                         "};" \n
                         "" \n
                         "//set Char ID" \n
                         "player setVariable[""CharacterID"",_charID];" \n
                         "" \n
                         "//Work out survival time" \n
                         "_totalMins = _survival select 0;" \n
                         "_days = floor (_totalMins / 1440);" \n
                         "_totalMins = (_totalMins - (_days * 1440));" \n
                         "_hours = floor (_totalMins / 60);" \n
                         "_mins =  (_totalMins - (_hours * 60));" \n
                         "" \n
                         "//player variables" \n
                         "BP_characterID =		_charID;" \n
                         "BP_hasFire = 			objNull;		//records players Fireplace object" \n
                         "BP_myCursorTarget = 	objNull;" \n
                         "BP_zombiesLocal = 	0;			//Used to record how many local zombies being tracked" \n
                         "BP_skilllevel = _days;  //total alive days" \n
                         "" \n
                         "//load in medical details" \n
                         "r_player_dead = 		player getVariable[""med_isDead"",false];" \n
                         "r_player_unconscious = 	player getVariable[""med_unconscious"", false];" \n
                         "r_player_infected =	player getVariable[""med_infected"",false];" \n
                         "r_player_injured = 	player getVariable[""med_injured"",false];" \n
                         "r_player_inpain = 		player getVariable[""med_inPain"",false];" \n
                         "r_player_cardiac = 	player getVariable[""med_isCardiac"",false];" \n
                         "r_player_lowblood =	player getVariable[""med_lowBlood"",false];" \n
                         "r_player_blood = 		player getVariable[""med_BloodQty"",r_player_bloodTotal];" \n
                         "" \n
                         "//Hunger/Thirst" \n
                         "_messing = player getVariable[""messing"",[0,0]];" \n
                         "BP_hunger = 	_messing select 0;" \n
                         "BP_thirst = _messing select 1;" \n
                         ""/*%FSM</STATEINIT""">*/;
